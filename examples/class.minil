// Class definition example in MiniL
protected class Calculator {
    int result;
    real pi;
    str operation;
    
    main {
        pi = 3.14159;
        result = 10;
        operation = "addition";
        
        if (result > 5) {
            result = result + 1;
        }
        
        while (result < 20) {
            result = result + 2;
        }
    }
}
